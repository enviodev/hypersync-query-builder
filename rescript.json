{
  "name": "vite-react-rescript-starter",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "uncurried": false,
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": [
    "rescript-vitest"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
